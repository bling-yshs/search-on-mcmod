{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_authors}"
    ],
    "contact": {
        "homepage": "${github_url}",
        "sources": "${github_url}"
    },
    "license": "${mod_license}",
    "icon": "logo.png",
    "environment": "*",
    "entrypoints": {
        "main": [
          "com.yshs.searchonmcmod.SearchOnMcmod"
        ]
    },
    "mixins": [
        "searchonmcmod.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": "*"
    },
    "suggests": {
        "another-mod": "*"
    }
}
